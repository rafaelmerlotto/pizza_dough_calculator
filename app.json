      "package": "com.rafaelmerlotto.PizzaDoughCalculator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "67844361-7dd0-4803-acef-0fbdf89219fc"
      }
